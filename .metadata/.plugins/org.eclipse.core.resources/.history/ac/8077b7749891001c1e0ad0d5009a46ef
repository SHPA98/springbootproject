package com.student;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class StudentApplicationmplements CommandLineRunner {

	@Autowired
	private UserRero Repo;
	
	public static void main(String[] args) {
		SpringApplication.run(StudentApplication.class, args);
	//System.out.println("pawansharma");
	//SpringApplication.run(SpringBootOracleDbApplication.class, args);
	
	}

	

	@Override
	public void run(String... args) throws Exception {
		// TODO Auto-generated method stub
		
		Repo.save(new User (20,"pawan"));
		Repo.save(new User (22,"sharma"));
		Repo.save(new User (1997,"subhm"));
		Repo.save(new User (24,"gupta"));
		
		 java.util.List<User> user = Repo.findAll();
		 
		 Iterator<User> itr=user.iterator();
		 while (itr.hasNext()) {
		
			 System.out.println(itr.next().toString());
		}
		 
	}
	
}
